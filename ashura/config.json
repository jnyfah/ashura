{
    "$schema": "./config.schema.json",
    "version": "0.0.1",
    "gpu.validation": false,
    "gpu.vsync": false,
    "gpu.preferences": [
        "dgpu",
        "vgpu",
        "igpu",
        "other",
        "cpu"
    ],
    "gpu.hdr": true,
    "gpu.buffering": 2,
    "gpu.antialiasing": "msaa",
    "gpu.msaa.level": 4,
    "window.resizable": true,
    "window.full_screen": false,
    "window.maximized": false,
    "window.borderless": false,
    "window.width": 1500,
    "window.height": 960,
    "shaders": {
        "VS:Blur/DownSample": "shaders/blur_downsample.vert.spv",
        "FS:Blur/DownSample": "shaders/blur_downsample.frag.spv",
        "VS:Blur/UpSample": "shaders/blur_upsample.vert.spv",
        "FS:Blur/UpSample": "shaders/blur_upsample.frag.spv",
        "VS:Ngon": "shaders/ngon.vert.spv",
        "FS:Ngon": "shaders/ngon.frag.spv",
        "VS:PBR": "shaders/pbr.vert.spv",
        "FS:PBR": "shaders/pbr.frag.spv",
        "VS:RRect": "shaders/rrect.vert.spv",
        "FS:RRect": "shaders/rrect.frag.spv"
    },
    "fonts": {
        "Amiri": "fonts/Amiri/Amiri-Regular.ttf",
        "MaterialIcons": "fonts/MaterialIcons/MaterialIcons-Regular.ttf",
        "Roboto": "fonts/Roboto/Roboto-Regular.ttf",
        "RobotoMono": "fonts/RobotoMono/RobotoMono-Regular.ttf"
    },
    "fonts.default": "RobotoMono",
    "images": {}
}